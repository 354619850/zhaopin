/**app.wxss**/

view,scroll-view{
	font-family: '微软雅黑';
	font-size: 28rpx;
}

icon{
	vertical-align: middle;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 

.placeholder-class{
	color:#b2b2b2;
}

._block{
	display: block;
}

._left{
	float:left;
}

._right{
	float:right;
}
