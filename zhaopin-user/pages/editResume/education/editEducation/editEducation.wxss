Page{
	background:#fff;
	box-sizing: border-box;
}

.box{
	padding:0px 30rpx;
}

.item{
	height:36px;
	border-bottom: 1rpx solid rgb(239,239,244);
}
.iconfont{
	line-height: 36px;
	vertical-align: middle;
	color:rgb(72,194,61);
}
.btn{
	font-size: 14px;
	margin-top:20px;
	padding-top:3px;
	padding-bottom: 3px;
	border-radius: 1px
}

.input{
	flex-grow: 1;
	line-height:36px;
	height:100%;
	vertical-align: middle;
	margin-left:8px;
}

